&quot;a&gt;.&quot;
msgstr &quot;&quot;
</value></props></cxt-def><cxt-def id="130" type="sdl:field" descr=".\templates\glos\servicegrid.html:29" purpose="Match"><props><value key="rawcontent">#: .\templates\glos\servicegrid.html:29
