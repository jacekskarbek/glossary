msgid &quot;serwisy internetowe&quot;
msgstr &quot;web services&quot;
</value></props></cxt-def><cxt-def id="199" type="sdl:field" descr=".\templates\locstar\website.html:36" purpose="Match"><props><value key="rawcontent">#: .\templates\locstar\website.html:36
msgid &quot;usługi online&quot;
msgstr &quot;online services&quot;
</value></props></cxt-def><cxt-def id="200" type="sdl:field" descr="invalid" purpose="Match"><props><value key="rawcontent">#~ msgid &quot;Trzy piętra, sala konferencyjna + 7 pokoi&quot;
#~ msgstr &quot;Three stories, conference room + 7 rooms&quot;
